@model IEnumerable<MalariaBackend.Models.Symptom>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Symptom_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Symptom_Description)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Sign_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Sign_Description)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.About_Malaria.Malaria_Description)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Symptom_Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Symptom_Description)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Sign_Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Sign_Description)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.About_Malaria.Malaria_Description)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Symptom_ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.Symptom_ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Symptom_ID })
        </td>
    </tr>
}

</table>
